{
    "name": "wonder-scale-website",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "dev": "ng serve --host 0.0.0.0 --ssl true --disableHostCheck",
        "start": "node dist/wonderscale-website/server/main.js",
        "build": "ng build --prod --aot --output-hashing=all",
        "build-testing": "ng build --configuration qa --aot --output-hashing=all",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "dev:ssr": "ng run wonder-scale-website:serve-ssr",
        "serve:ssr": "node dist/wonder-scale-website/server/main.js",
        "build:ssr": "ng build --configuration production && ng run wonder-scale-website:server:production",
        "build-testing:ssr": "ng build --configuration qa && ng run wonder-scale-website:server:qa",
        "prerender": "node ./seo/list-url-scripts.js --prod && ng run wonder-scale-website:prerender --routesFile ./seo/routes.txt && node ./seo/update-meta-data.js",
        "prerender-dev": "node ./seo/list-url-scripts.js --dev && ng run wonder-scale-website:prerender --routesFile ./seo/routes.txt && node ./seo/update-meta-data.js --dev",
        "prerender-testing": "node ./seo/list-url-scripts.js --testing && ng run wonder-scale-website:prerender && node ./seo/update-meta-data.js --testing",
        "deploy": "npm run build:ssr",
        "deploy-testing": "npm run build-testing:ssr"
    },
    "engines": {
        "node": "10.22.0",
        "npm": "6.1.0"
    },
    "private": true,
    "dependencies": {
        "@agm/core": "^1.1.0",
        "@angular/animations": "~9.0.2",
        "@angular/cdk": "^9.2.2",
        "@angular/common": "~9.0.2",
        "@angular/compiler": "~9.0.2",
        "@angular/core": "~9.0.2",
        "@angular/forms": "~9.0.2",
        "@angular/material": "^9.2.2",
        "@angular/platform-browser": "~9.0.2",
        "@angular/platform-browser-dynamic": "~9.0.2",
        "@angular/platform-server": "~9.0.2",
        "@angular/router": "~9.0.2",
        "@auth0/angular-jwt": "^4.0.0",
        "@nguniversal/express-engine": "^9.0.2",
        "@types/jquery": "^3.5.4",
        "angular-bootstrap-md": "^9.2.0",
        "angularx-social-login": "^3.0.1",
        "braintree-web": "^3.64.1",
        "croppie": "^2.6.4",
        "dom-to-image": "^2.6.0",
        "domino": "^2.1.6",
        "express": "^4.15.2",
        "jquery": "^3.5.1",
        "jquery.qrcode": "^1.0.3",
        "jwt-decode": "^2.2.0",
        "lodash": "^4.17.15",
        "moment": "^2.25.3",
        "ng-lazyload-image": "^7.1.0",
        "ngx-cookie-service": "^3.0.4",
        "ngx-facebook": "^3.0.0-0",
        "ngx-order-pipe": "^2.1.1",
        "ngx-progressbar": "^6.0.2",
        "ngx-swiper-wrapper": "^9.0.1",
        "prom-client": "^13.1.0",
        "rxjs": "~6.5.4",
        "tslib": "^1.10.0",
        "zone.js": "~0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.900.3",
        "@angular/cli": "~9.0.3",
        "@angular/compiler-cli": "~9.0.2",
        "@angular/language-service": "~9.0.2",
        "@nguniversal/builders": "^9.0.2",
        "@types/express": "^4.17.0",
        "@types/jasmine": "~3.5.0",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "^12.11.1",
        "codelyzer": "^5.1.2",
        "ftp-deploy": "^2.3.8",
        "jasmine-core": "~3.5.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~4.3.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~2.1.0",
        "karma-jasmine": "~2.0.1",
        "karma-jasmine-html-reporter": "^1.4.2",
        "protractor": "~5.4.3",
        "ts-node": "~8.3.0",
        "tslint": "~5.18.0",
        "typescript": "~3.7.5"
    }
}
