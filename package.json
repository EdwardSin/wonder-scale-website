{
  "name": "wonder-scale-website",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "dev": "ng serve --host 0.0.0.0 --ssl true --disableHostCheck",
    "start": "node dist/wonderscale-website/server/main.js",
    "build": "ng build --configuration production --aot --output-hashing=all",
    "build-testing": "ng build --configuration qa --aot --output-hashing=all",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run wonder-scale-website:serve-ssr",
    "serve:ssr": "node dist/wonder-scale-website/server/main.js",
    "build:ssr": "ng build --configuration production && ng run wonder-scale-website:server:production",
    "build-testing:ssr": "ng build --configuration qa && ng run wonder-scale-website:server:qa",
    "prerender": "node ./seo/list-url-scripts.js --configuration production && ng run wonder-scale-website:prerender --routesFile ./seo/routes.txt && node ./seo/update-meta-data.js",
    "prerender-dev": "node ./seo/list-url-scripts.js --dev && ng run wonder-scale-website:prerender --routesFile ./seo/routes.txt && node ./seo/update-meta-data.js --dev",
    "prerender-testing": "node ./seo/list-url-scripts.js --testing && ng run wonder-scale-website:prerender && node ./seo/update-meta-data.js --testing",
    "deploy": "npm run build:ssr",
    "deploy-testing": "npm run build-testing:ssr"
  },
  "engines": {
    "node": "10.22.0",
    "npm": "6.1.0"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.1.0",
    "@angular/animations": "~12.2.2",
    "@angular/cdk": "^12.2.2",
    "@angular/common": "~12.2.2",
    "@angular/compiler": "~12.2.2",
    "@angular/core": "~12.2.2",
    "@angular/forms": "~12.2.2",
    "@angular/material": "^12.2.2",
    "@angular/platform-browser": "~12.2.2",
    "@angular/platform-browser-dynamic": "~12.2.2",
    "@angular/platform-server": "~12.2.2",
    "@angular/router": "~12.2.2",
    "@auth0/angular-jwt": "^4.0.0",
    "@nguniversal/express-engine": "^9.0.2",
    "@types/jquery": "^3.5.4",
    "angular-bootstrap-md": "^9.2.0",
    "angularx-social-login": "^3.0.1",
    "braintree-web": "^3.64.1",
    "croppie": "^2.6.4",
    "dom-to-image": "^2.6.0",
    "domino": "^2.1.6",
    "express": "^4.15.2",
    "jquery": "^3.5.1",
    "jquery.qrcode": "^1.0.3",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.15",
    "moment": "^2.25.3",
    "ng-lazyload-image": "^7.1.0",
    "ngx-cookie-service": "^3.0.4",
    "ngx-facebook": "^3.0.0-0",
    "ngx-order-pipe": "^2.1.1",
    "ngx-progressbar": "^6.0.2",
    "prom-client": "^13.1.0",
    "rxjs": "~6.5.4",
    "swiper": "^6.8.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.2",
    "@angular/cli": "~12.2.2",
    "@angular/compiler-cli": "~12.2.2",
    "@angular/language-service": "~12.2.2",
    "@nguniversal/builders": "^9.0.2",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "ftp-deploy": "^2.3.8",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5"
  }
}